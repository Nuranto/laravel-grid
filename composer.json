{
    "name": "nuranto/laravel-grid",
    "description": "Personnal version of abandonned package leantony/laravel-grid",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "leantony",
        "bootstrap",
        "laravel-grid",
        "laravel",
        "pjax",
        "grid"
    ],
    "homepage": "https://github.com/nuranto/laravel-grid",
    "authors": [{
            "name": "Antony [leantony] Chacha",
            "email": "chachaantony@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Vincent Enjalbert",
            "email": "chef@web-cooking.net",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/view": "~7|~8",
        "illuminate/support": "~7|~8",
        "illuminate/console": "~7|~8",
        "illuminate/contracts": "~7|~8",
        "illuminate/http": "~7|~8",
        "illuminate/database": "~7|~8",
        "illuminate/pagination": "~7|~8"
    },
    "suggest": {
        "maatwebsite/excel": "For exporting grid data as excel (~3.0)",
        "barryvdh/laravel-dompdf": "For exporting grid data as PDF (^0.8.2)"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~3.0",
        "orchestra/testbench-dusk": "^3.5",
        "phpunit/php-code-coverage": "^6.0|^7.05",
        "barryvdh/laravel-dompdf": "^0.8",
        "maatwebsite/excel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Leantony\\Grid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leantony\\Grid\\Providers\\GridServiceProvider"
            ],
            "aliases": {
                "Modal": "Leantony\\Grid\\Facades\\Modal"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}